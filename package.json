{
  "name": "react-money-component",
  "version": "1.4.6",
  "description": "React.js Money Component",
  "main": "lib/money.js",
  "scripts": {
    "test": "jest",
    "lib": "gulp",
    "build": "npm run lib",
    "prepublish": "npm run test && npm run build"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "src/money.js": true
    },
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "author": "Music Glue",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/musicglue/react-money-component.git"
  },
  "bugs": {
    "url": "https://github.com/musicglue/react-money-component/issues"
  },
  "homepage": "https://github.com/musicglue/react-money-component",
  "keywords": [
    "money",
    "react",
    "js-money"
  ],
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "js-money": "^0.6.0",
    "accounting": "^0.4.1",
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "coveralls": "^2.11.4",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "istanbul": "^0.4.0",
    "istanbul-coveralls": "^1.0.3",
    "jest-cli": "^14.1.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "js-money": "^0.6.0",
    "accounting": "^0.4.1"
  }
}
